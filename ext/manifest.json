{
  "manifest_version": 2,

  "name": "Google Music - Skip Thumb Down",
  "description": "This extension skips thumb-down tracks from the queue on Google Music",
  "version": "0.0.1"

  ,"icons": {
    "48": "icon48.png"
    ,"128": "icon128.png"
  }

  ,"browser_action": {
  }

  ,"permissions": [
    "activeTab"
    ,"https://play.google.com/music/"
  ]

  ,"content_scripts": [

    {
      "matches": [ "https://play.google.com/music/*" ]
      ,"js": [ "content.js" ]
      ,"run_at": "document_end"
    }
  ]

}
